<ehcache:config
  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
  xmlns:ehcache='http://www.ehcache.org/v3'
  xsi:schemaLocation=" http://www.ehcache.org/v3 ../../../main/resources/ehcache-core.xsd">

  <ehcache:service>
    <ehcache:serializer-default>
      <ehcache:serializer>com.pany.ehcache.serializer.TestSerializer</ehcache:serializer>
    </ehcache:serializer-default>
  </ehcache:service>


  <ehcache:cache alias="bar" storeByValueOnHeap="true">
    <ehcache:key-type>java.lang.String</ehcache:key-type>
    <ehcache:value-type>java.lang.String</ehcache:value-type>
    <ehcache:heap size="10"/>
  </ehcache:cache>
</ehcache:config>